<mvc:View
	controllerName="com.myorg.myUI5App.controller.Home"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
	<Page
		id="pageTiles"
		title=""
		showNavButton="true"
		navButtonPress=".onNavBack"
	>
		<customHeader>
			<Bar>
				<contentRight>
					<Image
						src="icon/logo-sirx2.png"
						height="100%"
						width="auto"
					/>
				</contentRight>
				<contentLeft>
					<Button
						icon="sap-icon://nav-back"
						press="onNavBack"
					/>
				</contentLeft>
			</Bar>
		</customHeader>
		<content>
			<FlexBox 
					width="auto"
					height="100%"
					justifyContent="Center"
					alignItems="Center"
			>
				<FlexBox
					width="auto"
					wrap="Wrap"
					justifyContent="Center"
					items="{
						path: 'home>/tiles',
						factory: '.factoryTiles'
					}"
				/>			
			</FlexBox>
		</content>
	</Page>
</mvc:View>